      WaveGenerator1 336 264
       description '<Description>
   <Version>4.0</Version>
<IsMainModel>1</IsMainModel>
 <KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Sources\WaveGenerator-Square.emx</LibraryPath>
  <TimeStamp>2009-3-5 16:05:33</TimeStamp>
</Description>';
       type 'WaveGenerator-Square'
        ports
         signal out output;
         signal in frequency;
       end;
       icon bg bottom
        figures
         rectangle 320.1 248 351.9 280 color 0 fill 15132390;
         line 324.1 251.9 323.9 278.2 color 0 fill 0;
         line 321.9 276.1 347.9 276.1 color 0 fill 0;
         line 324.1 276.1 328 276.1 328 264 color 16711680 fill 0;
         line 336 264 328 264 336 264 color 16711680 fill 0;
         line 336 276.1 336 264 336 276.1 color 16711680 fill 0;
         line 344 264 344 276.1 344 264 color 16711680 fill 0;
         line 347.9 264.1 343.7 264.1 color 16711680 fill 0;
         line 336 276.1 344 276.1 color 16711680 fill 0;
       end;
       implementation eq
parameters
	real amplitude = 1.0;			// amplitude of the wave

variables					
	real hidden s, s2;
	boolean hidden change;
equations
	"calculate the square wave"
	s = sign (sin (frequency/2 * time));
	s2 = sign(sin (2*frequency/2* time));
	
	if( s > 0 && s2 > 0 ) then
		output = 0;
	else 
	if( s2 <= 0 && s > 0) then
		output =	24;
	else 
	if( s <= 0  && s2<= 0) then
		output =	-24;
	else 
		output = 0;
	end;
	end;
	end;
implementation_end;
      WaveGenerator2 336 376
       description '<Description>
   <Version>4.0</Version>
<IsMainModel>1</IsMainModel>
 <KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Sources\WaveGenerator-Square.emx</LibraryPath>
