       type 'WaveGenerator-Square'
        ports
         signal out output;
         signal in frequency;
       end;
       icon bg bottom
        figures
         rectangle 320.1 360 351.9 392 color 0 fill 15132390;
         line 324.1 363.9 323.9 390.2 color 0 fill 0;
         line 321.9 388.1 347.9 388.1 color 0 fill 0;
         line 324.1 388.1 328 388.1 328 376 color 16711680 fill 0;
         line 336 376 328 376 336 376 color 16711680 fill 0;
         line 336 388.1 336 376 336 388.1 color 16711680 fill 0;
         line 344 376 344 388.1 344 376 color 16711680 fill 0;
         line 347.9 376.1 343.7 376.1 color 16711680 fill 0;
         line 336 388.1 344 388.1 color 16711680 fill 0;
       end;
       implementation eq
parameters
	real amplitude = 1.0;			// amplitude of the wave

variables					
	real hidden s, s2;
	boolean hidden change;
equations

	"calculate at least 2 points per period 
	(just after the change in sign)"

	"calculate the square wave"
	s = sign (sin (frequency/2 * time));
	s2 = sign(sin (2*frequency/2* time));
	
	if( s > 0 && s2 > 0 ) then
		output = 24;
	else 
	if( s2 <= 0 && s > 0) then
		output =	0;
	else 
	if( s <= 0  && s2<= 0) then
		output =	0;
	else 
		output = -24;
	end;
	end;
	end;
implementation_end;
