</Description>';
       type 'WaveGenerator-Square'
        ports
         signal out output;
         signal in frequency;
       end;
       icon bg bottom
        figures
         rectangle 272.1 336 303.9 368 color 0 fill 15132390;
         line 276.1 339.9 275.9 366.2 color 0 fill 0;
         line 273.9 364.1 299.9 364.1 color 0 fill 0;
         line 276.1 364.1 280 364.1 280 352 color 16711680 fill 0;
         line 288 352 280 352 288 352 color 16711680 fill 0;
         line 288 364.1 288 352 288 364.1 color 16711680 fill 0;
         line 296 352 296 364.1 296 352 color 16711680 fill 0;
         line 299.9 352.1 295.7 352.1 color 16711680 fill 0;
         line 288 364.1 296 364.1 color 16711680 fill 0;
       end;
       implementation eq
parameters
	real amplitude = 1.0;			// amplitude of the wave

variables					
	real hidden s, s2;
	boolean hidden change;
equations

	"calculate at least 2 points per period 
	(just after the change in sign)"

	"calculate the square wave"
	s = sign (sin (frequency/2 * time));
	s2 = sign(sin (2*frequency/2* time));
	
	if( s > 0 && s2 > 0 ) then
		output = 1;
	else 
	if( s2 <= 0 && s > 0) then
		output =	0;
	else 
	if( s <= 0  && s2<= 0) then
		output =	0;
	else 
		output = -1;
	end;
	end;
	end;
implementation_end;
