      WaveGenerator1 288 200
       description '<Description>
   <Version>4.0</Version>
<IsMainModel>1</IsMainModel>
 <KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Sources\WaveGenerator-Square.emx</LibraryPath>
  <TimeStamp>2009-3-5 16:05:33</TimeStamp>
</Description>';
       type 'WaveGenerator-Square'
        ports
         signal out output;
         signal in frequency;
       end;
       icon bg bottom
        figures
         rectangle 272.1 184 303.9 216 color 0 fill 15132390;
         line 276.1 187.9 275.9 214.2 color 0 fill 0;
         line 273.9 212.1 299.9 212.1 color 0 fill 0;
         line 276.1 212.1 280 212.1 280 200 color 16711680 fill 0;
         line 288 200 280 200 288 200 color 16711680 fill 0;
         line 288 212.1 288 200 288 212.1 color 16711680 fill 0;
         line 296 200 296 212.1 296 200 color 16711680 fill 0;
         line 299.9 200.1 295.7 200.1 color 16711680 fill 0;
         line 288 212.1 296 212.1 color 16711680 fill 0;
       end;
       implementation eq
parameters
	real amplitude = 1.0;			// amplitude of the wave

variables					
	real hidden s, s2;
	boolean hidden change;
equations
	"calculate the square wave"
	s = sign (sin (frequency/2 * time));
	s2 = sign(sin (2*frequency/2* time));
	
	if( s > 0 && s2 > 0 ) then
		output = 0;
	else 
	if( s2 <= 0 && s > 0) then
		output =	1;
	else 
	if( s <= 0  && s2<= 0) then
		output =	-1;
	else 
		output = 0;
	end;
	end;
	end;
implementation_end;
      WaveGenerator2 288 352
       description '<Description>
   <Version>4.0</Version>
<IsMainModel>1</IsMainModel>
 <KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Sources\WaveGenerator-Square.emx</LibraryPath>
